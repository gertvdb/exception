{
  "name": "gertvdb/exception",
  "description": "Module for handling user visible exceptions",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "minimum-stability": "alpha",
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "php": ">=8.1",
    "drupal/onlyone": "^2.0",
    "gertvdb/settings": "^2.0.0"
  },
  "require-dev": {
    "drupal/coder": "^8.3",
    "drupal/core-recommended": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\exception\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
